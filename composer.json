{
  "name": "libelulasoft/yii2-common-helpers",
  "description": "Common helpers para yii2 y mongodb",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "helpers"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Johnny Garcia",
      "email": "johnnygar98@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "~2.0.41",
    "mongodb/mongodb": "^1.8.0",
    "yiisoft/yii2-mongodb": "^2.0"
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Libelulasoft\\CommonHelpers\\": "src"
    }
  }
}
